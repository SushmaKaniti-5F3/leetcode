class Solution {
public:
    int majorityElement(vector<int>& nums) {
        int n=nums.size(),c;
        vector<int>v(n+1,0);
        sort(nums.begin(),nums.end());
        for(int i=0;i<n;i++)
        {
            c=0;
           if(nums[i]==nums[i+1])
           {
               v[i]++;
           }
        }
        return m;
    }
};
