class Solution {
public:
    void reverseString(vector<char>& s) {
        int n=s.size();
        int j=n-1,i=0;
        char a;
        while(i<=j)
        {
            a=s[i];
            s[i]=s[j];
            s[j]=a;  
            i++;
            j--;
        }
       for(auto it:s)
           cout<<it;
    }
};
