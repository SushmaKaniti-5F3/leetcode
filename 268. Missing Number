class Solution {
public:
    int missingNumber(vector<int>& nums) {
        int n=nums.size(),a;
        int m=*max_element(nums.begin(),nums.end());
        vector<int>d(m+1,0);
        for(int i=0;i<n;i++)
        {
           d[nums[i]]++;
        }
        for(int i=0;i<m;i++)
        {
            if(d[i]==0)
            {
                a=i;
            }
        }
        return a;
    }
};
