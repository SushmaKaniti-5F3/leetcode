class Solution {
public:
    bool backspaceCompare(string s, string t) {
        int n1=s.size();
        int n2=t.size();
        stack<int>s1;
        stack<int>s2;
        for(int i=0;i<n1;i++)
        {
            if(s[i]=='#')
            {
                if(!s1.empty())
                s1.pop();
            }
            else
            s1.push(s[i]);
        }
        for(int i=0;i<n2;i++)
        { 
            if(t[i]=='#')
            {
                if(!s2.empty())
                s2.pop();
            }
            else
            s2.push(t[i]);
        }
        if(s1.size()==s2.size())
        {
        while(!s1.empty())
        {
            if(s1.top()!=s2.top())
            {
                return false;
            }
            else
            {
                s1.pop();
                s2.pop();
            }
        }
        return true;
        }
        return false;
    }
};
